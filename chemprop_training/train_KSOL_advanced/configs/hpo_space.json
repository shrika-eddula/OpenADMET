{
  "search_space": {
    "hidden_size": {
      "type": "categorical",
      "values": [1024, 1300, 1600, 2048, 2400]
    },
    "depth": {
      "type": "int",
      "low": 3,
      "high": 7
    },
    "ffn_num_layers": {
      "type": "int",
      "low": 2,
      "high": 4
    },
    "dropout": {
      "type": "float",
      "low": 0.0,
      "high": 0.3,
      "step": 0.05
    },
    "batch_size": {
      "type": "categorical",
      "values": [32, 64, 128]
    },
    "max_lr": {
      "type": "loguniform",
      "low": 0.0001,
      "high": 0.01
    },
    "epochs": {
      "type": "int",
      "low": 50,
      "high": 150,
      "step": 10
    },
    "warmup_epochs": {
      "type": "int",
      "low": 2,
      "high": 10
    },
    "weight_decay": {
      "type": "loguniform",
      "low": 0.000001,
      "high": 0.001
    },
    "augmentation_factor": {
      "type": "int",
      "low": 0,
      "high": 20,
      "step": 5
    }
  },
  "optimization": {
    "metric": "val_mae",
    "direction": "minimize",
    "n_trials": 100,
    "timeout": 86400,
    "n_jobs": 1,
    "pruner": "median",
    "pruner_kwargs": {
      "n_startup_trials": 10,
      "n_warmup_steps": 20,
      "interval_steps": 5
    }
  }
}